        {
          "dataset": "iot_data2",
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${DS_TIMESCALEDB}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "SELECT\n  AVG(humidity) as Avg,\n  time_bucket('1 minutes', \"time\") AS \"time\",\n  sensor_data.device_id,\n  MAX(humidity) as Max,\n  MIN(humidity) as Min\nFROM\n  sensor_data\nWHERE sensor_data.device_id = '$device_id' \nGROUP BY\n  sensor_data.device_id,\n  time_bucket('1 minutes', \"time\")\nORDER BY\n  time_bucket('1 minutes', \"time\")",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "name": "AVG",
                "parameters": [
                  {
                    "name": "humidity",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "parameters": [
                  {
                    "name": "\"time\"",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "parameters": [
                  {
                    "name": "device_id",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "name": "MAX",
                "parameters": [
                  {
                    "name": "humidity",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "name": "MIN",
                "parameters": [
                  {
                    "name": "humidity",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "name": "device_id",
                  "type": "string"
                },
                "type": "groupBy"
              },
              {
                "property": {
                  "name": "\"time\"",
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "sensor_data"
        }
      ],
      "title": "Humidade",
      "type": "timeseries"
    },
